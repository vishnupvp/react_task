		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"paths": {
			"components/*": ["src/components/*"],
			"router/*": ["src/router/*"],
			"pages/*": ["src/pages/*"],
			"assets/*": ["src/assets/*"],
			"redux/*": ["src/redux/*"]
		}
	}
}
